{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**/bin/**/*.dll"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Wrap",
      "_appName": "Wrap",
      "_appFooter": "<span>Copyright (c) 2019-2026 <a href=\"https://bfren.dev\">bfren</a>. Made with <a href=\"https://dotnet.github.io/docfx\">docfx</a></span>"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
